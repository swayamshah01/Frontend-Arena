{
  "name": "front-arena",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run optimize-video && vite build",
    "build:vercel": "vite build",
    "optimize-video": "node optimize-video.mjs",
    "lint": "eslint .",
    "preview": "vite preview",
    "video:optimize": "node optimize-video.mjs",
    "video:check": "node -e \"console.log('FFmpeg:', require('@ffmpeg-installer/ffmpeg').path); console.log('FFprobe:', require('@ffprobe-installer/ffprobe').path);\"",
    "video:clean": "rm -f public/video.mp4",
    "postbuild": "echo 'Build completed successfully'"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@react-three/drei": "^10.0.5",
    "@react-three/fiber": "^9.1.0",
    "@studio-freight/lenis": "^1.0.42",
    "@tailwindcss/vite": "^4.0.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "framer-motion": "^12.5.0",
    "gsap": "^3.12.7",
    "install": "^0.13.0",
    "lenis": "^1.2.3",
    "lucide-react": "^0.483.0",
    "npm": "^11.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^7.4.0",
